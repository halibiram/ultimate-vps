{
  "name": "ultimate-vps",
  "version": "1.0.0",
  "description": "Ultimate VPS SSH Manager",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:seed": "ts-node prisma/seed-test-user.ts",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:create": "prisma migrate dev --create-only"
  },
  "keywords": [
    "ssh",
    "vps",
    "manager",
    "proxy"
  ],
  "author": "Ultimate VPS Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "8.4.0",
    "@fastify/jwt": "7.2.4",
    "@fastify/redis": "6.1.1",
    "@fastify/static": "6.12.0",
    "@fastify/websocket": "^10.0.1",
    "@prisma/client": "5.6.0",
    "bcryptjs": "2.4.3",
    "dotenv": "^16.3.1",
    "fastify": "4.24.3",
    "node-cron": "3.0.3",
    "node-pty": "^1.0.0",
    "socket.io": "4.7.4",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "20.8.10",
    "@types/ws": "^8.5.10",
    "jest": "^30.1.3",
    "nodemon": "3.0.1",
    "prisma": "5.6.0",
    "ts-jest": "^29.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
